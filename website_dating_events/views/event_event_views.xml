<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
 
        <record id="event_event_dating_view_form" model="ir.ui.view">
            <field name="name">Event Dating Form View</field>
            <field name="model">event.event</field>
            <field name="inherit_id" ref="event.view_event_form"/>
            <field name="arch" type="xml">
      	        <field name="organizer_id" position="after">
      	            <field name="dating"/>
      	        </field>
      	        <notebook position="inside">
      	            <page string="Dating" attrs="{'invisible': [('dating','=',False)]}">
      	                <group>
      	                    <field name="current_males"/>
      	                    <field name="max_males"/>
                            <field name="current_males"/>
      	                    <field name="max_females"/>
      	                    <field name="dating_description"/>
      	                    <field name="event_feedback_ids"/>
      	                    <field name="max_attendee_distance"/>
      	                    <field name="dating_filter_ids">
      	                        <tree>
      	                            <field name="name"/>
      	                            <field name="domain"/>
      	                        </tree>
      	                    </field>
      	                </group>
      	            </page>
      	        </notebook>
      	    </field>
        </record>

    </data>
</openerp>