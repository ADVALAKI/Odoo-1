<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seo_metric_page_load_time" model="sem.metric">
            <field name="name">Page Load Time</field>
            <field name="function_name">page_load_time</field>
            <field name="description">Simulates a page load using Google Chrome and returns the time between navigationStart and domComplete events.</field>
            <field name="depend_ids" eval="[(6, 0, [ref('seo_depend_selenium')])]"/>
            <field name="active">1</field>
        </record>

        <record id="seo_metric_http_requests" model="sem.metric">
            <field name="name">HTTP Requests</field>
            <field name="function_name">http_requests</field>
            <field name="description">Number of Files that are loaded</field>
            <field name="depend_ids" eval="[(6, 0, [ref('seo_depend_selenium')])]"/>
            <field name="active">1</field>
        </record>

    </data>
</odoo>