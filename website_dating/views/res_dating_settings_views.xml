<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_dating_settings_form" model="ir.ui.view">
        <field name="name">res.dating.settings.form</field>
        <field name="model">res.dating.settings</field>
        <field name="arch" type="xml">
      	    <form>
      	        <group>
      	            <field name="min_dating_age"/>
